# Model 5
model5 <- lmer(ZLOG_RESIDUALS_NFL_MEAN_WS ~CPMS_INTEN_V3 + SQRTCRP_V3 + AGE_V3 + CHARLSON_V3 + OPIOIDMED_CAT + CPMS_INTEN_V3:SQRTCRP_V3 + (1 | CASE), data = VETSA, REML = FALSE, control = lmerControl(optimizer="bobyqa"))
